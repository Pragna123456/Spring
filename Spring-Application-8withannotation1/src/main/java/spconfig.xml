<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>
    
    <bean id="perminateAddress" class="com.sathya.address.AddressBean">
        <property name="hno" value="120"/>
        <property name="city" value="hyderabad"/>
        <property name="state" value="telangana"/>
    </bean>
    
    <bean id="temparayAddress" class="com.sathya.address.AddressBean">
        <property name="hno" value="101"/>
        <property name="city" value="vijayawada"/>
        <property name="state" value="Andhra Pradesh"/>
    </bean>
    
    <bean id="empObj" class="com.sathya.address.EmployeeBean">
        <property name="empId" value="1200"/>
        <property name="empName" value="Suman"/>
    </bean>

</beans>
